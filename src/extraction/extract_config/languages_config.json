{
  "languages": {
    "default_config": {
      "max_depth": 10,
      "extract_comments": true,
      "extract_imports": true,
      "extract_variables": false
    },
    "python": {
      "extensions": [
        "py",
        "pyx",
        "pyi"
      ]
    },
    "javascript": {
      "extensions": [
        "js",
        "jsx",
        "mjs"
      ]
    },
    "typescript": {
      "extensions": [
        "ts",
        "tsx"
      ]
    },
    "java": {
      "extensions": [
        "java"
      ]
    },
    "cpp": {
      "extensions": [
        "cpp",
        "cc",
        "cxx",
        "hpp",
        "h"
      ]
    },
    "go": {
      "extensions": [
        "go"
      ]
    },
    "rust": {
      "extensions": [
        "rs"
      ]
    },
    "csharp": {
      "extensions": [
        "cs"
      ]
    },
    "tcl": {
      "extensions": [
        "tcl",
        "itcl"
      ]
    }
  }
}