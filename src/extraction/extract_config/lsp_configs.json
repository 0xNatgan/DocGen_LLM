{
    "languages": {
        "python": {
            "extensions": [
                ".py",
                ".pyi",
                ".pyx"
            ],
            "lsp_server": {
                "name": "Pylsp",
                "command": "pylsp",
                "args": [
                    "--verbose"
                ],
                "install_command": "pip install 'python-lsp-server[all]'",
                "check_command": [
                    "pylsp",
                    "--version"
                ],
                "description": "Pylsp - Full Python language support with semantic tokens"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```python\\n(.*?)\\n```",
                    "def\\s+([^:]+):",
                    "class\\s+([^:]+):"
                ],
                "skip_patterns": [
                    "^\\s*#",
                    "^\\s*\"\"\"",
                    "^\\s*'''",
                    "^\\s*\\("
                ]
            }
        },
        "javascript": {
            "extensions": [
                ".js",
                ".mjs",
                ".jsx"
            ],
            "imports": [
                "import",
                "require"
            ],
            "lsp_server": {
                "command": "typescript-language-server",
                "args": [
                    "--stdio"
                ],
                "install_command": "npm install -g typescript-language-server typescript",
                "check_command": [
                    "typescript-language-server",
                    "--version"
                ],
                "description": "TypeScript Language Server - JavaScript/TypeScript support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```(?:javascript|typescript)\\n(.*?)\\n```",
                    "function\\s+([^{]+)",
                    "class\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*\\("
                ]
            }
        },
        "typescript": {
            "extensions": [
                ".ts",
                ".tsx"
            ],
            "imports": [
                "import",
                "require"
            ],
            "lsp_server": {
                "command": "typescript-language-server",
                "args": [
                    "--stdio"
                ],
                "install_command": "npm install -g typescript-language-server typescript",
                "check_command": [
                    "typescript-language-server",
                    "--version"
                ],
                "description": "TypeScript Language Server - Full TypeScript support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```typescript\\n(.*?)\\n```",
                    "function\\s+([^{]+)",
                    "class\\s+([^{]+)",
                    "interface\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*\\("
                ]
            }
        },
        "java": {
            "extensions": [
                ".java"
            ],
            "imports": [
                "import",
                "package"
            ],
            "lsp_server": {
                "command": "jdtls",
                "args": [],
                "install_command": "npm install -g jdtls",
                "check_command": [
                    "jdtls",
                    "--version"
                ],
                "description": "Java Language Server - NPM version"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```java\\n(.*?)\\n```",
                    "```\\n(.*?)\\n```"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*@"
                ]
            }
        },
        "go": {
            "extensions": [
                ".go"
            ],
            "imports": [
                "import"
            ],
            "lsp_server": {
                "command": "gopls",
                "args": [],
                "install_command": "go install golang.org/x/tools/gopls@latest",
                "check_command": [
                    "gopls",
                    "version"
                ],
                "description": "Go Language Server (gopls) - Official Go support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```go\\n(.*?)\\n```",
                    "func\\s+([^{]+)",
                    "type\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*"
                ]
            }
        },
        "rust": {
            "extensions": [
                ".rs"
            ],
            "imports": [
                "use",
                "mod"
            ],
            "lsp_server": {
                "command": "rust-analyzer",
                "args": [],
                "install_command": "rustup component add rust-analyzer",
                "check_command": [
                    "rust-analyzer",
                    "--version"
                ],
                "description": "Rust Analyzer - Official Rust language support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```rust\\n(.*?)\\n```",
                    "fn\\s+([^{]+)",
                    "struct\\s+([^{]+)",
                    "impl\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*"
                ]
            }
        },
        "cpp": {
            "extensions": [
                ".cpp",
                ".cc",
                ".cxx",
                ".hpp",
                ".h",
                ".hxx"
            ],
            "imports": [
                "#include",
                "using namespace",
                "namespace",
                "using"
            ],
            "lsp_server": {
                "command": "clangd",
                "args": [],
                "install_command": "apt install clangd-12 # or brew install llvm on macOS",
                "check_command": [
                    "clangd",
                    "--version"
                ],
                "description": "Clangd - C/C++ language support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```(?:cpp|c\\+\\+)\\n(.*?)\\n```",
                    "\\w+\\s+\\w+\\s*\\([^)]*\\)",
                    "class\\s+([^{]+)",
                    "struct\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*#"
                ]
            }
        },
        "csharp": {
            "extensions": [
                ".cs"
            ],
            "imports": [
                "using",
                "namespace"
            ],
            "lsp_server": {
                "name": "OmniSharp-LSP",
                "installation_type": "docker",
                "docker_image": "gendoc-ai/lsp-omnisharp:latest"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```csharp\\n(.*?)\\n```",
                    "public\\s+([^{]+)",
                    "private\\s+([^{]+)",
                    "class\\s+([^{]+)",
                    "interface\\s+([^{]+)",
                    "struct\\s+([^{]+)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*\\[",
                    "^\\s*using\\s+",
                    "^\\s*namespace\\s+"
                ]
            }
        },
        "c": {
            "extensions": [
                ".c",
                ".h"
            ],
            "imports": [
                "#include"
            ],
            "lsp_server": {
                "command": "clangd",
                "args": [],
                "install_command": "apt install clangd-12 # or brew install llvm on macOS",
                "check_command": [
                    "clangd",
                    "--version"
                ],
                "description": "Clangd - C language support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```c\\n(.*?)\\n```",
                    "\\w+\\s+\\w+\\s*\\([^)]*\\)"
                ],
                "skip_patterns": [
                    "^\\s*//",
                    "^\\s*/\\*",
                    "^\\s*\\*",
                    "^\\s*#"
                ]
            }
        },
        "fortran": {
            "extensions": [
                ".f90",
                ".f95",
                ".f03",
                ".f08",
                ".f77",
                ".f",
                ".for",
                ".com"
            ],
            "imports": [
                "use",
                "include"
            ],
            "lsp_server": {
                "command": "fortls",
                "args": [],
                "install_command": "pip install fortls",
                "check_command": [
                    "fortls",
                    "--version"
                ],
                "description": "Fortran Language Server (fortls) - Fortran support"
            },
            "signature_extraction": {
                "hover_patterns": [
                    "```fortran\\n(.*?)\\n```",
                    "\\w+\\s+\\w+\\s*\\([^)]*\\)"
                ],
                "skip_patterns": [
                    "^\\s*!",
                    "^\\s*\\!"
                ]
            }
        }
    },
    "symbol_kind": [5, 6, 7, 8, 9, 10, 11, 12, 23, 24, 25],
    "kind_to_types": {
        "1": "file",
        "2": "module",
        "3": "namespace",
        "4": "package",
        "5": "class",
        "6": "method",
        "7": "property",
        "8": "field",
        "9": "constructor",
        "10": "enum",
        "11": "interface",
        "12": "function",
        "13": "variable",
        "14": "constant",
        "15": "string",
        "16": "number",
        "17": "boolean",
        "18": "array",
        "19": "object",
        "20": "key",
        "21": "null",
        "22": "enum_member",
        "23": "struct",
        "24": "event",
        "25": "operator",
        "26": "type_parameter"
    }
}