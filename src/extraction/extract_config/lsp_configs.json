{
    "languages": {
        "python": {
            "extensions": [
                ".py",
                ".pyi",
                ".pyx"
            ],
            "lsp_server": {
                "name": "pyright",
                "command": "pyright-langserver",
                "args": [
                    "--stdio"
                ],
                "install_command": "npm install -g pyright",
                "check_command": [
                    "pyright",
                    "--version"
                ],
                "description": "Pyright - Full Python language support with semantic tokens"
            }
        },
        "javascript": {
            "extensions": [
                ".js",
                ".mjs",
                ".jsx"
            ],
            "lsp_server": {
                "command": "typescript-language-server",
                "args": [
                    "--stdio"
                ],
                "install_command": "npm install -g typescript-language-server typescript",
                "check_command": [
                    "typescript-language-server",
                    "--version"
                ],
                "description": "TypeScript Language Server - JavaScript/TypeScript support"
            }
        },
        "typescript": {
            "extensions": [
                ".ts",
                ".tsx"
            ],
            "lsp_server": {
                "command": "typescript-language-server",
                "args": [
                    "--stdio"
                ],
                "install_command": "npm install -g typescript-language-server typescript",
                "check_command": [
                    "typescript-language-server",
                    "--version"
                ],
                "description": "TypeScript Language Server - Full TypeScript support"
            }
        },
        "java": {
            "extensions": [
                ".java"
            ],
            "lsp_server": {
                "command": "jdtls",
                "args": [],
                "install_command": "npm install -g jdtls",
                "check_command": [
                    "jdtls",
                    "--version"
                ],
                "description": "Java Language Server - NPM version"
            }
        },
        "go": {
            "extensions": [
                ".go"
            ],
            "lsp_server": {
                "command": "gopls",
                "args": [],
                "install_command": "go install golang.org/x/tools/gopls@latest",
                "check_command": [
                    "gopls",
                    "version"
                ],
                "description": "Go Language Server (gopls) - Official Go support"
            }
        },
        "rust": {
            "extensions": [
                ".rs"
            ],
            "lsp_server": {
                "command": "rust-analyzer",
                "args": [],
                "install_command": "rustup component add rust-analyzer",
                "check_command": [
                    "rust-analyzer",
                    "--version"
                ],
                "description": "Rust Analyzer - Official Rust language support"
            }
        },
        "cpp": {
            "extensions": [
                ".cpp",
                ".cc",
                ".cxx",
                ".hpp",
                ".h",
                ".hxx"
            ],
            "lsp_server": {
                "command": "clangd",
                "args": [],
                "install_command": "apt install clangd-12 # or brew install llvm on macOS",
                "check_command": [
                    "clangd",
                    "--version"
                ],
                "description": "Clangd - C/C++ language support",
                "docker_image": "clangd-lsp:latest"
            }
        },
        "csharp": {
            "extensions": [
                ".cs"
            ],
            "lsp_server": {
                "name": "OmniSharp-LSP",
                "docker_image": "gendoc-ai/lsp-omnisharp:latest"
            }
        },
        "c": {
            "extensions": [
                ".c",
                ".h"
            ],
            "lsp_server": {
                "command": "clangd",
                "args": [],
                "install_command": "apt install clangd-12 # or brew install llvm on macOS",
                "check_command": [
                    "clangd",
                    "--version"
                ],
                "description": "Clangd - C language support",
                "docker_image": "clangd-lsp:latest"
            }
        },
        "fortran": {
            "extensions": [
                ".f90",
                ".f95",
                ".f03",
                ".f08",
                ".f77",
                ".f",
                ".for",
                ".com"
            ],
            "lsp_server": {
                "command": "fortls",
                "args": [],
                "install_command": "pip install fortls",
                "check_command": [
                    "fortls",
                    "--version"
                ],
                "description": "Fortran Language Server (fortls) - Fortran support"
            }
        },
        "tcl": {
            "extensions": [
                ".tcl",
                ".itcl"
            ],
            "lsp_server": {
                "name": "TCL-LSP",
                "command": "tclsh",
                "args": [
                    "/usr/local/bin/tcl-lsp.tcl",
                    "--stdio"
                ],
                "docker_image": "tcl-lsp:latest",
                "language_id": "tcl",
                "description": "TCL Language Server - Tcl/Tk support",
                "initializationOptions": {}
            },
            "language_id": "tcl"
        }
    },
    "symbol_kind": [
        5,
        6,
        7,
        8,
        9,
        10,
        11,
        12,
        23,
        24,
        25
    ],
    "kind_to_types": {
        "1": "file",
        "2": "module",
        "3": "namespace",
        "4": "package",
        "5": "class",
        "6": "method",
        "7": "property",
        "8": "field",
        "9": "constructor",
        "10": "enum",
        "11": "interface",
        "12": "function",
        "13": "variable",
        "14": "constant",
        "15": "string",
        "16": "number",
        "17": "boolean",
        "18": "array",
        "19": "object",
        "20": "key",
        "21": "null",
        "22": "enum_member",
        "23": "struct",
        "24": "event",
        "25": "operator",
        "26": "type_parameter"
    }
}